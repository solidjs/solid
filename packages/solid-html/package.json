{
  "name": "@solidjs/html",
  "description": "Build-less Tagged-Template-Literal Templating for Solid",
  "version": "2.0.0-experimental.4",
  "author": "Ryan Carniato",
  "license": "MIT",
  "homepage": "https://solidjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidjs/solid-html"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/html.cjs",
  "module": "./dist/html.js",
  "types": "./types/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "types",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/html.js",
      "require": "./dist/html.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "build": "npm-run-all -nl build:*",
    "build:clean": "rimraf dist/ coverage/",
    "build:js": "rollup -c",
    "types": "npm-run-all -nl types:*",
    "types:clean": "rimraf types/",
    "types:html": "tsc --project ./tsconfig.json && ncp ../../node_modules/lit-dom-expressions/types/index.d.ts ./types/lit.d.ts"
  },
  "peerDependencies": {
    "@solidjs/web": "^2.0.0-experimental.4"
  },
  "devDependencies": {
    "@solidjs/web": "workspace:*"
  }
}
