    "jsdom": "^21.1.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "lit-dom-expressions": "0.35.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.5",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "seroval": "^0.7.0",
    "symlink-dir": "^5.0.1",
    "tsconfig-replace-paths": "^0.0.11",
    "turbo": "^1.3.1",
    "typescript": "^4.9.5",
    "vite-plugin-solid": "^2.6.1",
    "vitest": "^0.29.3"
