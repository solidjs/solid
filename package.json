{
  "name": "solid-build",
  "description": "A declarative JavaScript library for building user interfaces.",
  "version": "0.15.4",
  "author": "Ryan Carniato",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansolid/solid"
  },
  "main": "lib/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "private": true,
  "scripts": {
    "postinstall": "lerna bootstrap --hoist --no-ci",
    "clean": "npm run clean:artifacts && npm run clean:packages && npm run clean:root",
    "clean:artifacts": "lerna run clean --parallel",
    "clean:packages": "lerna clean --yes",
    "clean:root": "rimraf node_modules",
    "test": "lerna run test --concurrency=1 --stream",
    "test:coverage": "lerna run test:coverage --parallel",
    "build": "lerna run build --concurrency=1 --stream",
    "publish:release": "lerna run build && lerna publish",
    "report:coverage": "lerna run report:coverage --parallel"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-typescript": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@types/requestidlecallback": "^0.3.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-jsx-dom-expressions": "~0.14.12",
    "coveralls": "^3.0.9",
    "dom-expressions": "0.14.10",
    "hyper-dom-expressions": "~0.14.0",
    "jest": "~24.9.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "lerna": "^3.19.0",
    "lit-dom-expressions": "~0.14.0",
    "ncp": "2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.9",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.7.4"
  }
}
